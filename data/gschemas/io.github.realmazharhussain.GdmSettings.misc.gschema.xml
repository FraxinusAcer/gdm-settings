<schemalist gettext-domain="gdm-settings">
  <schema id="io.github.realmazharhussain.GdmSettings.misc"
       path="/io/github/realmazharhussain/GdmSettings/misc/">
    
    <key name="enable-welcome-message" type="b">
      <default>false</default>
    </key>

    <key name="welcome-message" type="s">
      <default>"Welcome!"</default>
    </key>

    <key name="enlarge-welcome-message" type="b">
      <default>true</default>
    </key>

    <key name="enable-logo" type="b">
      <default>false</default>
    </key>

    <key name="logo" type="s">
      <default>""</default>
    </key>

    <key name="disable-restart-buttons" type="b">
      <default>false</default>
    </key>

    <key name="disable-user-list" type="b">
      <default>false</default>
    </key>

  </schema>
</schemalist>
